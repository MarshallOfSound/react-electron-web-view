  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "electron": "^1.4.4",
    "electron-mocha": "^3.1.1",
    "enzyme": "^2.5.1",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.9.0",
    "react": "^0.14.0",
    "react-addons-test-utils": "^15.3.2",
